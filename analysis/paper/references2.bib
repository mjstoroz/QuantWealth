  author = {Budd, Chelsea and Bogucki, Peter and Lillie, Malcolm and Grygiel, Ryszard and Lorkiewicz, Wies{\l}aw and Schulting, Rick},
  year = {2020},
  month = aug,
  journal = {Antiquity},
  volume = {94},
  number = {376},
  pages = {932--947},
  issn = {0003-598X, 1745-1744},
  doi = {10.15184/aqy.2020.102},
  abstract = {Abstract , Understanding socioeconomic inequality is fundamental for studies of societal development in European prehistory. This article presents dietary ({$\delta$} 13 C and {$\delta$} 15 N) isotope values for human and animal bone collagen from Early Neolithic Os\l onki 1 in north-central Poland ( c. 4600\textendash 4100 cal BC). A new series of AMS radiocarbon determinations show that, of individuals interred at the same time, those with copper artefacts exhibit significantly higher {$\delta$} 13 C values than those without. The authors' results suggest a link between high-status goods and intra-community differences in diet and/or preferential access to the agropastoral landscape.},
  langid = {english},
  keywords = {\#duplicate-citation-key}
}

