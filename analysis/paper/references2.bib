@article{rasmussen_etal15,
  title = {Early {{Divergent Strains}} of {{Yersinia}} Pestis in {{Eurasia}} 5,000 {{Years Ago}}},
  author = {Rasmussen, Simon and Allentoft, Morten Erik and Nielsen, Kasper and Orlando, Ludovic and Sikora, Martin and Sj{\"o}gren, Karl-G{\"o}ran and Pedersen, Anders Gorm and Schubert, Mikkel and Van Dam, Alex and Kapel, Christian Moliin Outzen and Nielsen, Henrik Bj{\o}rn and Brunak, S{\o}ren and Avetisyan, Pavel and Epimakhov, Andrey and Khalyapin, Mikhail Viktorovich and Gnuni, Artak and Kriiska, Aivar and Lasak, Irena and Metspalu, Mait and Moiseyev, Vyacheslav and Gromov, Andrei and Pokutta, Dalia and Saag, Lehti and Varul, Liivi and Yepiskoposyan, Levon and {Sicheritz-Pont{\'e}n}, Thomas and Foley, Robert A. and Lahr, Marta Miraz{\'o}n and Nielsen, Rasmus and Kristiansen, Kristian and Willerslev, Eske},
  year = {2015},
  month = oct,
  journal = {Cell},
  volume = {163},
  number = {3},
  pages = {571--582},
  issn = {0092-8674},
  doi = {10.1016/j.cell.2015.10.009},
  abstract = {The bacteria Yersinia pestis is the etiological agent of plague and has caused human pandemics with millions of deaths in historic times. How and when it originated remains contentious. Here, we report the oldest direct evidence of Yersinia pestis identified by ancient DNA in human teeth from Asia and Europe dating from 2,800 to 5,000 years ago. By sequencing the genomes, we find that these ancient plague strains are basal to all known Yersinia pestis. We find the origins of the Yersinia pestis lineage to be at least two times older than previous estimates. We also identify a temporal sequence of genetic changes that lead to increased virulence and the emergence of the bubonic plague. Our results show that plague infection was endemic in the human populations of Eurasia at least 3,000 years before any historical recordings of pandemics.},
  langid = {english},
  pmid = {26496604},
  keywords = {\#duplicate-citation-key},
  file = {C\:\\Mikkel\\Zotero library\\storage\\LQFWRGNP\\Rasmussen m.fl. - 2015 - Early Divergent Strains of Yersinia pestis in Eura.pdf;C\:\\Mikkel\\Zotero library\\storage\\Z9I4KZYP\\S0092-8674(15)01322-7.html}
}

