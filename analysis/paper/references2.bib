@article{irvin_lundock20,
  title = {Purification {{Through Puppies}}: {{Dog Symbolism}} and {{Sacrifice}} in the {{Mediterranean World}}},
  shorttitle = {Purification {{Through Puppies}}},
  author = {Irvin, Aaron and Lundock, Jason},
  year = {2020},
  abstract = {Berkeley Electronic Press Selected Works, The process of forming identity is not a linear process but is built upon the multiple roles that one has in relation to family, real or imagined ancestry, previous and current professions, religious affiliation and any number of social or civil allegiances.\&nbsp;\&nbsp; This could make for a somewhat heterogeneous and multi-layered identity in a society as complex as that of the Roman world.\&nbsp;\&nbsp; It is just such complexity in the process of building and communicating identity that has led to the term `Romanization' falling out of use; though it has, interestingly enough, not led to a departure from the conflict between `Roman' and `native' peoples and cultures being the predominant paradigm in conceptualizing cultural interaction during the Roman period.\&nbsp; This paper begins with one particular artistic representation, canine imagery in Roman Britain, that defies attempts to easily categorize it as either native or Roman, and indeed defies even categorization as either regional or local, universal or idiosyncratic.\&nbsp; As will be shown, the invocation of canine imagery and canine related rituals invokes a series of thematically linked traditions from across the Mediterranean world.\&nbsp; The presence of this imagery in the highly diverse environment of Roman Britain thus created an artistic expression that was simultaneously universal, as well as unique to particular ritual expression, thus defying any attempt to categorize its presentation as either uniquely Roman or `native', or as an expression of Romanitas and therefore a byproduct of Romanization.},
  langid = {english},
  keywords = {\#duplicate-citation-key},
  file = {C\:\\Mikkel\\Zotero library\\storage\\QQ9YD9BS\\10.html}
}

