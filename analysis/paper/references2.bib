@article{piliciauskas_etal20,
  title = {{{ANCIENT LOGBOATS IN LITHUANIA}}: {{NEW FINDS}}, {{WOOD TAXA AND CHRONOLOGY}}},
  shorttitle = {{{ANCIENT LOGBOATS IN LITHUANIA}}},
  author = {Pili{\v c}iauskas, Gytis and Pranck{\.e}nait{\.e}, Elena and Peseckas, K{\k{e}}stutis and Ma{\v z}eika, Jonas and Matuzevi{\v c}i{\=u}t{\.e}, Simona},
  year = {2020},
  month = oct,
  journal = {radiocarbon. an international journal of cosmogenic isotope research},
  volume = {62},
  number = {5},
  pages = {1299--1315},
  issn = {0033-8222, 1945-5755},
  doi = {10.1017/RDC.2020.65},
  abstract = {ABSTRACT More than 70 logboats are known in Lithuania and about half of them date before 1800 cal AD. This paper reviews these more ancient logboats, which were discovered in the beds of former or still existing lakes and rivers and present new radiocarbon ( 14 C) dates and wood taxa identifications. Special attention is paid to the logboat found at \v{S}ventoji 58, which is at present the oldest 14 C-dated (2895\textendash 2640 cal BC) logboat found East of the Baltic Sea. Most logboats from the pre-1800s period were 14 C dated to medieval and early modern periods and they were made out of pine and oak wood. From a very scarce dataset available it seems that oak wood was introduced into logboat building by Neolithic people, who also brought new technologies in stone tool production. We also noted that 14 C dating does not confirm the logboat typology suggested in earlier studies.},
  langid = {english},
  keywords = {\#duplicate-citation-key}
}

